<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox fillWidth="true" prefHeight="711.0" prefWidth="945.0" xmlns:fx="http://javafx.com/fxml" fx:controller="de.saxsys.javafx.workshop.example.charts.ChartsView">
  <children>
    <TextField id="c" fx:id="textField" prefHeight="35.0" prefWidth="959.0" promptText="Bitte geben Sie einen Text ein." style="-fx-font-size: 16px;" />
    <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
        <PieChart id="PieChart" fx:id="pieChart" legendSide="BOTTOM" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <BarChart fx:id="barChart" alternativeColumnFillVisible="true" alternativeRowFillVisible="true" animated="false" barGap="2.0" categoryGap="5.0" horizontalGridLinesVisible="true" horizontalZeroLineVisible="true" legendVisible="false" prefHeight="-1.0" prefWidth="-1.0" verticalGridLinesVisible="true" verticalZeroLineVisible="true">
          <xAxis>
            <CategoryAxis id="categoryAxis" animated="true" side="BOTTOM" />
          </xAxis>
          <yAxis>
            <NumberAxis id="numberAxis" animated="false" autoRanging="false" side="LEFT" upperBound="30.0" />
          </yAxis>
        </BarChart>
      </children>
    </HBox>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
